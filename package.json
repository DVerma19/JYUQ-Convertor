{
  "name": "jyuq-convertor",
  "displayName": "jyuq-convertor",
  "description": "Extension to Convert Quantum Code into other Quantum Programming Languages",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:jyuq-convertor.helloWorld",
    "onCommand:jyuq.conversions"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jyuq-convertor.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "jyuq.conversions",
        "title": "Conversion of Quantum Circuit"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "jyuq.conversions",
          "group": "navigation",
          "title": "Open the Conversion",
          "when": "editorTextFocus"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "path": "^0.12.7",
    "quantum-circuit": "^0.9.204"
  }
}
